@{

    System.Data.DataSet ds = (System.Data.DataSet)ViewData["ds"];

}


<table>
    <tr>
        <td style="height:50px;">
            <div style="color: #3F4F70;font-weight:bold">共有未处理振动光纤异常报警:&nbsp<font color="red"> @(ViewBag.Vibfibcount)</font> &nbsp;个 </div>

        </td>
    </tr>
    <tr>

        <td>
            <table class="realtb">

                <tr align="center">
                    <th>&nbsp;&nbsp;&nbsp;&nbsp;</th>
                    <th><b>编号</b></th>
                    <th><b>报警时间</b></th>
                    <th><b>通道号</b></th>
                    <th width="140px" height="40px"><b>报警位置(米)</b></th>
                    <th><b>光纤状态</b></th>
                    <th width="200px"><b>动作</b></th>
                </tr>
                @{
                    foreach (System.Data.DataRow dr in ds.Tables[0].Rows)
                    {
                        <tr>
                            <td height="40px">&nbsp;</td>
                            <td width="60px">@dr["id"]</td>
                            <td width="280px">@dr["push_time"]</td>
                            <td  width="100px">@dr["channel_id"]</td>
                            <td width="200px">@dr["fiber_bk_len"]</td>
                            <td width="160px">@dr["fiber_stat"]</td>
                            <td><a class="norbn" href="#" onclick="vib_fib_confirm('@(dr["id"])', 1)">确认</a>&nbsp;&nbsp;<a class="norbn" href="#" onclick="vib_fib_confirm('@(dr["id"])', 2)">误报</a></td>

                        </tr>

                    }
                    ds.Dispose();

                }

            </table>

        </td>

    </tr>

</table>


